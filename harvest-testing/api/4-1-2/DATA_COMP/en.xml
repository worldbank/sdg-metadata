          &lt;/mrow&gt;
          &lt;mrow&gt;
            &lt;mi&gt;a&lt;/mi&gt;
            &lt;mo&gt;+&lt;/mo&gt;
            &lt;mn&gt;3&lt;/mn&gt;
            &lt;mi&gt;t&lt;/mi&gt;
            &lt;mn&gt;5&lt;/mn&gt;
          &lt;/mrow&gt;
        &lt;/msub&gt;
      &lt;/mrow&gt;
