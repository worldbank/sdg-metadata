                &lt;mi&gt;t&lt;/mi&gt;
                &lt;mn&gt;5&lt;/mn&gt;
              &lt;/mrow&gt;
            &lt;/msub&gt;
          &lt;/mrow&gt;
        &lt;/msub&gt;
      &lt;/mrow&gt;
    &lt;/mfrac&gt;
  &lt;/math&gt;&lt;/p&gt;
&lt;p&gt;where:&lt;/p&gt;
&lt;p&gt;&lt;math xmlns=&quot;http://www.w3.org/1998/Math/MathML&quot;&gt;
    &lt;msub&gt;
      &lt;mrow&gt;
        &lt;mi&gt;C&lt;/mi&gt;
        &lt;mi&gt;R&lt;/mi&gt;
      &lt;/mrow&gt;
      &lt;mrow&gt;
        &lt;mi&gt;n&lt;/mi&gt;
      &lt;/mrow&gt;
    &lt;/msub&gt;
    &lt;mo&gt;=&lt;/mo&gt;
  &lt;/math&gt; completion rate for level n of education&lt;/p&gt;
&lt;p&gt;&lt;math xmlns=&quot;http://www.w3.org/1998/Math/MathML&quot;&gt;
    &lt;msub&gt;
      &lt;mrow&gt;
        &lt;mi&gt;P&lt;/mi&gt;
      &lt;/mrow&gt;
      &lt;mrow&gt;
        &lt;msub&gt;
